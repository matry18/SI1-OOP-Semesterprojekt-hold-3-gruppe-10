<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="200.0" prefWidth="435.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.AskingForHelpController">
   <children>
      <Label fx:id="lblHelpOutPlayer" layoutX="10.0" layoutY="10.0" text="WILL YOU HELP OUT THE OTHER PLAYER?" textFill="#56171a">
         <font>
            <Font name="Windlass" size="18.0" />
         </font>
      </Label>
      <Button fx:id="btnYes" layoutY="31.0" mnemonicParsing="false" onAction="#handleYesButtonAction" prefHeight="169.0" prefWidth="217.5" text="MORE YES THAN NO" textAlignment="CENTER" textFill="#56171a">
         <font>
            <Font name="Windlass" size="16.0" />
         </font>
      </Button>
      <Button fx:id="btnNo" layoutX="218.0" layoutY="31.0" mnemonicParsing="false" onAction="#handleNoButtonAction" prefHeight="169.0" prefWidth="217.5" text="MORE NO THAN YES" textAlignment="CENTER" textFill="#56171a">
         <font>
            <Font name="Windlass" size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
