<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="150.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.LosingController">
    <children>
        <TextArea fx:id="txtAreaLosing" editable="false" prefHeight="150.0" prefWidth="600.0" wrapText="true">
         <font>
            <Font name="Garamond Bold" size="14.0" />
         </font>
      </TextArea>
      <Button fx:id="btnQuitFromLosinge" alignment="BOTTOM_CENTER" contentDisplay="CENTER" layoutX="264.0" layoutY="125.0" mnemonicParsing="false" onAction="handleQuitButtonAction" text="Quit game" textAlignment="CENTER" textOverrun="CENTER_ELLIPSIS">
          <font>
            <Font name="Windlass" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
