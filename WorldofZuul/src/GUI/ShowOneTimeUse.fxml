   <children>
      <ImageView fx:id="imgShowOneTimeUse" fitHeight="600.0" fitWidth="378.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="btnCloseWindow" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleCloseWindowButtonAction" opacity="0.0" prefHeight="600.0" prefWidth="378.0" text="Close window" textAlignment="CENTER">
         <font>
            <Font name="Windlass" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
